{
  "name": "auroral-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --ssl true --ssl-cert ./ssl/localhost.crt --ssl-key ./ssl/localhost.key",
    "dev": "ng serve -o --configuration=dev --ssl true --ssl-cert ./ssl/localhost.crt --ssl-key ./ssl/localhost.key",
    "staging": "node set-env.js develop && ng serve -o --configuration=staging --ssl true --ssl-cert ./ssl/localhost.crt --ssl-key ./ssl/localhost.key",
    "build": "ng build",
    "build:staging": "ng build --configuration=staging",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:test": "prettier **/*.{ts,html,md,scss,json} --list-different",
    "format:write": "prettier **/*.{ts,html,md,scss,json} --write",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer ./dist/auroral-fe/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^7.0.1",
    "@angular/animations": "~13.0.2",
    "@angular/cdk": "^13.0.2",
    "@angular/common": "~13.0.2",
    "@angular/compiler": "~13.0.2",
    "@angular/core": "~13.0.2",
    "@angular/forms": "~13.0.2",
    "@angular/material": "^13.0.2",
    "@angular/material-moment-adapter": "^11.2.13",
    "@angular/platform-browser": "~13.0.2",
    "@angular/platform-browser-dynamic": "~13.0.2",
    "@angular/router": "~13.0.2",
    "@auth0/angular-jwt": "^5.0.2",
    "angular-file": "^3.6.0",
    "date-fns": "^2.27.0",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "moment": "^2.29.4",
    "ng-qrcode": "^6.0.0",
    "ngx-mat-select-search": "^4.0.2",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.2",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "^3.10.2",
    "@types/node": "^12.20.37",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "^2.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.4.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
